<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="user">

   <!-- 根据id查询用户 -->
   <select id="findUserById" parameterType="int"
             resultType="User">
        SELECT * FROM USER WHERE id = #{id}
   </select>
   
</mapper>