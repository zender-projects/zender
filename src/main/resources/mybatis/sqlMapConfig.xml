<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
   
    
    <typeAliases>
        <typeAlias type="com.zender.domain.User" alias="User"/>
        <typeAlias type="com.zender.domain.resume.Employment" alias="Employment"/>
        <typeAlias type="com.zender.domain.resume.Skill" alias="Skill"/>
